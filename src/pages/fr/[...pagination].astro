---
import { computePaths } from "../../i18n/utils";
import MainPage from "../../layouts/MainPage.astro";

export async function getStaticPaths() {
  const globFr = import.meta.glob(`../../content/blog/fr/*.md`);

  return computePaths("fr", Object.keys(globFr).length);
}

const { pagination } = Astro.params;
---

<MainPage numeroPage={pagination} />
